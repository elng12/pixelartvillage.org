{
  "name": "pixel-art-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "node scripts/embed-en-locale.cjs && node scripts/gen-social-preview.cjs && node scripts/gen-showcase.cjs",
    "dev": "node scripts/dev.cjs",
    "prebuild": "node scripts/embed-en-locale.cjs && node scripts/gen-social-preview.cjs && node scripts/gen-showcase.cjs && node scripts/generate-sitemap.cjs && node scripts/gen-pseo-og.cjs",
    "build": "vite build && node scripts/prerender-spa.cjs",
    "postbuild": "node scripts/inline-critical-css.cjs && node scripts/seo-check.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "lint:ci": "eslint src scripts --max-warnings=0",
    "test": "playwright test",
    "test:unit": "node scripts/unit-tests.js",
    "test:ui": "playwright test --ui",
    "verify:dist": "node scripts/verify-dist-entries.cjs",
    "seo:check": "node scripts/seo-check.js",
    "seo:sitemap": "node scripts/generate-sitemap.js",
    "seo:keywords": "node scripts/keyword-frequency-analysis.js",
    "seo:report": "node scripts/seo-keyword-report.js",
    "seo:density": "node scripts/kw-density-check.js",
    "seo:adjust": "node scripts/keyword-adjustment-strategy.js",
    "seo:reduce-art": "node scripts/reduce-art-keyword.js",
    "seo:phrases": "node scripts/phrase-density-analysis.js",
    "seo:optimize-phrases": "node scripts/phrase-density-optimizer.js",
    "seo:boost-phrases": "node scripts/phrase-booster.js",
    "seo:inject-phrases": "node scripts/target-phrase-injector.js",
    "seo:massive-inject": "node scripts/massive-phrase-injection.js",
    "seo:verify-ranking": "node scripts/verify-target-ranking.js",
    "i18n:check": "node scripts/i18n-check.js",
    "i18n:pseudo": "node scripts/gen-pseudo-locale.js",
    "seo:kw": "node scripts/kw-density-check.js",
    "seo:kw:report": "node scripts/kw-density-report.js",
    "seo:kw:words": "node scripts/kw-density-words.js",
    "seo:kw:single": "node scripts/kw-density-single.js",
    "seo:kw:en:text": "node scripts/kw-density-single.js --route /en/ --tsv",
    "seo:kw:en:meta": "node scripts/kw-density-single.js --route /en/ --include-meta --tsv",
    "seo:kw:en:meta:hyphen": "node scripts/kw-density-single.js --route /en/ --include-meta --split-hyphen --tsv",
    "seo:kw:en:full": "node scripts/kw-density-single.js --route /en/ --include-meta --include-jsonld --split-hyphen --tsv",
    "seo:kw:site:full": "node scripts/kw-density-site-full.js",
    "seo:kw:root:full": "node scripts/kw-density-single.js --route / --include-meta --include-jsonld --include-attrs --split-hyphen --tsv",
    "seo:kw:root:yourtool": "node scripts/kw-density-single.js --route / --include-meta --include-jsonld --include-attrs --split-hyphen --dupe-text --stem --token-letters-only --tsv --verbose",
    "sitemap:verify": "node scripts/verify-sitemap-no-en.cjs"
  },
  "dependencies": {
    "i18next": "^25.5.2",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.3",
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@playwright/test": "^1.48.2",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "critters": "^0.0.23",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "sharp": "^0.34.4",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.6"
  },
  "engines": {
    "node": ">=18 <22"
  }
}
